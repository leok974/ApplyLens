{
  "uid": "applylens-agent-v2",
  "title": "ApplyLens â€“ Agent V2 Overview",
  "description": "Mailbox Agent V2 performance and usage metrics",
  "schemaVersion": 39,
  "version": 1,
  "tags": ["applylens", "agent-v2", "mailbox-agent"],
  "timezone": "browser",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "refresh": "30s",
  "templating": {
    "list": [
      {
        "name": "DS_PROM",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "selected": true,
          "text": "Prometheus",
          "value": "Prometheus"
        },
        "hide": 0,
        "refresh": 1,
        "label": "Prometheus Datasource"
      },
      {
        "name": "intent",
        "type": "query",
        "datasource": "${DS_PROM}",
        "query": "label_values(mailbox_agent_runs_total, intent)",
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "hide": 0,
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "label": "Intent Filter"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Agent Runs per Minute (by intent)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": "${DS_PROM}",
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": ["mean", "max"]
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (intent) (rate(mailbox_agent_runs_total{status=\"done\"}[5m]))",
          "legendFormat": "{{intent}}"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Agent Run Duration (p50, p95, p99)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": "${DS_PROM}",
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 0
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "p99" },
            "properties": [
              {
                "id": "color",
                "value": { "fixedColor": "red", "mode": "fixed" }
              }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": ["lastNotNull"]
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.50, sum by (le) (rate(mailbox_agent_run_duration_seconds_bucket{intent=~\"$intent\"}[5m])))",
          "legendFormat": "p50"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(mailbox_agent_run_duration_seconds_bucket{intent=~\"$intent\"}[5m])))",
          "legendFormat": "p95"
        },
        {
          "refId": "C",
          "expr": "histogram_quantile(0.99, sum by (le) (rate(mailbox_agent_run_duration_seconds_bucket{intent=~\"$intent\"}[5m])))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "id": 3,
      "type": "bargauge",
      "title": "Tool Usage by Type (last 1h)",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 8 },
      "datasource": "${DS_PROM}",
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 10, "color": "yellow" },
              { "value": 50, "color": "orange" }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        },
        "showUnfilled": true
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (tool) (increase(mailbox_agent_tool_calls_total{status=\"success\"}[1h]))",
          "legendFormat": "{{tool}}"
        }
      ]
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Total RAG Contexts Used (last 1h)",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 8 },
      "datasource": "${DS_PROM}",
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "blue" },
              { "value": 100, "color": "green" },
              { "value": 500, "color": "yellow" }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": {
          "calcs": ["sum"],
          "values": false
        },
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(mailbox_agent_rag_context_count[1h]))",
          "legendFormat": "RAG contexts"
        }
      ]
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Redis Cache Hit Ratio",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 8 },
      "datasource": "${DS_PROM}",
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 0.5, "color": "yellow" },
              { "value": 0.8, "color": "green" }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        },
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(mailbox_agent_redis_hits_total{result=\"hit\"}[5m])) / (sum(rate(mailbox_agent_redis_hits_total{result=\"hit\"}[5m])) + sum(rate(mailbox_agent_redis_hits_total{result=\"miss\"}[5m])))",
          "legendFormat": "Hit Ratio"
        }
      ]
    },
    {
      "id": 6,
      "type": "piechart",
      "title": "RAG Context Sources (last 1h)",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 8 },
      "datasource": "${DS_PROM}",
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "values": ["value", "percent"]
        },
        "pieType": "donut",
        "reduceOptions": {
          "calcs": ["sum"],
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (source) (increase(mailbox_agent_rag_context_count[1h]))",
          "legendFormat": "{{source}}"
        }
      ]
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Tool Call Success Rate",
      "gridPos": { "h": 4, "w": 8, "x": 20, "y": 8 },
      "datasource": "${DS_PROM}",
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (tool) (rate(mailbox_agent_tool_calls_total{status=\"success\"}[5m])) / sum by (tool) (rate(mailbox_agent_tool_calls_total[5m]))",
          "legendFormat": "{{tool}}"
        }
      ]
    },
    {
      "id": 8,
      "type": "table",
      "title": "Agent Run Summary (last 6h)",
      "gridPos": { "h": 8, "w": 20, "x": 0, "y": 12 },
      "datasource": "${DS_PROM}",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Total Runs" },
            "properties": [
              {
                "id": "custom.width",
                "value": 120
              }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Avg Duration (s)" },
            "properties": [
              {
                "id": "custom.width",
                "value": 150
              },
              {
                "id": "unit",
                "value": "s"
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Total Runs"
          }
        ]
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (intent) (increase(mailbox_agent_runs_total{status=\"done\"}[6h]))",
          "legendFormat": "",
          "format": "table",
          "instant": true
        },
        {
          "refId": "B",
          "expr": "avg by (intent) (rate(mailbox_agent_run_duration_seconds_sum[6h]) / rate(mailbox_agent_run_duration_seconds_count[6h]))",
          "legendFormat": "",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true
            },
            "renameByName": {
              "intent": "Intent",
              "Value #A": "Total Runs",
              "Value #B": "Avg Duration (s)"
            }
          }
        }
      ]
    },
    {
      "id": 9,
      "type": "stat",
      "title": "Error Rate (last 5m)",
      "gridPos": { "h": 8, "w": 4, "x": 20, "y": 12 },
      "datasource": "${DS_PROM}",
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 0.01, "color": "yellow" },
              { "value": 0.05, "color": "orange" },
              { "value": 0.1, "color": "red" }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        },
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(mailbox_agent_runs_total{status=\"error\"}[5m])) / sum(rate(mailbox_agent_runs_total[5m]))",
          "legendFormat": "Error Rate"
        }
      ]
    }
  ]
}
