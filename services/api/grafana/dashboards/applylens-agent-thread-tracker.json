              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "horizontal",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "showPercentageChange": false
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 18
      }
    },
    {
      "id": 7,
      "type": "stat",
      "title": "Interviews – runs, thread_lists, clicks (24h)",
      "description": "Funnel metrics for the 'interviews' intent over the last 24 hours.",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(applylens_agent_runs_total{intent=\"interviews\"}[24h]))",
