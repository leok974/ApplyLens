{
  "title": "ApplyLens – Agent & Thread Viewer",
  "uid": "applylens-agent-thread",
  "tags": ["applylens", "agent", "thread-viewer", "tracker"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
  },
  "templating": {
    "list": [
      {
        "name": "DS_PROM",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "selected": true,
          "text": "Prometheus",
          "value": "prometheus"
        },
        "hide": 0,
        "refresh": 1,
        "label": "Prometheus Datasource"
      }
    ]
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Agent runs by intent (per minute)",
      "description": "Rate of agent runs per minute, grouped by intent (followups, bills, interviews, etc.).\n\nMetric: applylens_agent_runs_total",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (intent) (rate(applylens_agent_runs_total[5m]))",
          "legendFormat": "{{intent}}",
          "interval": "",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0,
          "custom": {
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto",
            "spanNulls": false,
            "axisPlacement": "auto",
            "drawStyle": "line"
          },
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": ["mean", "max", "last"]
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Thread list coverage (scan intents)",
      "description": "Percentage of agent runs that return thread_list cards with threads, for scan intents only.\n\nFormula: (thread_list_returned / agent_runs) * 100\n\nIntents: followups, unsubscribe, clean_promos, bills, interviews, suspicious",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(sum by (intent) (rate(applylens_agent_threadlist_returned_total{intent=~\"followups|unsubscribe|clean_promos|bills|interviews|suspicious\"}[1h])) / sum by (intent) (rate(applylens_agent_runs_total{intent=~\"followups|unsubscribe|clean_promos|bills|interviews|suspicious\"}[1h]))) * 100",
          "legendFormat": "{{intent}}",
          "interval": "",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto",
            "spanNulls": false,
            "axisPlacement": "auto",
            "drawStyle": "line"
          },
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 50,
                "color": "yellow"
              },
              {
                "value": 80,
                "color": "green"
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": ["mean", "last"]
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Thread → Tracker clicks (per minute)",
      "description": "Rate of clicks on 'Open in Tracker' from Thread Viewer, grouped by intent.\n\nMetric: applylens_agent_thread_to_tracker_click_total",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (intent) (rate(applylens_agent_thread_to_tracker_click_total[5m]))",
          "legendFormat": "{{intent}}",
          "interval": "",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0,
          "custom": {
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto",
            "spanNulls": false,
            "axisPlacement": "auto",
            "drawStyle": "line"
          },
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": ["mean", "max", "last"]
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 9
      }
    },
    {
      "id": 4,
      "type": "table",
      "title": "Click-through rate: thread_list → tracker (scan intents, 24h)",
      "description": "Click-through rate for each scan intent over the last 24 hours.\n\nCTR = (thread_to_tracker_clicks / agent_runs) * 100\n\nIntents: followups, unsubscribe, clean_promos, bills, interviews, suspicious",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (intent) (increase(applylens_agent_thread_to_tracker_click_total{intent=~\"followups|unsubscribe|clean_promos|bills|interviews|suspicious\"}[24h]))",
          "legendFormat": "{{intent}}",
          "interval": "",
          "format": "table",
          "instant": true
        },
        {
          "refId": "B",
          "expr": "sum by (intent) (increase(applylens_agent_runs_total{intent=~\"followups|unsubscribe|clean_promos|bills|interviews|suspicious\"}[24h]))",
          "legendFormat": "{{intent}}",
          "interval": "",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "intent"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 150
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Clicks"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 100
              },
              {
                "id": "decimals",
                "value": 0
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Runs"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 100
              },
              {
                "id": "decimals",
                "value": 0
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "CTR (%)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percent"
              },
              {
                "id": "decimals",
                "value": 2
              },
              {
                "id": "custom.width",
                "value": 120
              },
              {
                "id": "custom.displayMode",
                "value": "gradient-gauge"
              },
              {
                "id": "min",
                "value": 0
              },
              {
                "id": "max",
                "value": 100
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "value": null,
                      "color": "red"
                    },
                    {
                      "value": 10,
                      "color": "yellow"
                    },
                    {
                      "value": 25,
                      "color": "green"
                    }
                  ]
                }
              }
            ]
          }
        ]
      },
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "job": true,
              "instance": true
            },
            "indexByName": {},
            "renameByName": {
              "Value #A": "Clicks",
              "Value #B": "Runs"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "alias": "CTR (%)",
            "binary": {
              "left": "Clicks",
              "operator": "/",
              "right": "Runs"
            },
            "reduce": {
              "reducer": "sum"
            },
            "replaceFields": false
          }
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "CTR (%)",
            "desc": true
          }
        ]
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 9
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Followups – runs, thread_lists, clicks (24h)",
      "description": "Funnel metrics for the 'followups' intent over the last 24 hours.\n\n1. Agent runs\n2. Thread lists returned (with threads)\n3. Clicks to tracker\n\nThis provides a rough conversion funnel.",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(applylens_agent_runs_total{intent=\"followups\"}[24h]))",
          "legendFormat": "Runs",
          "interval": "",
          "format": "time_series"
        },
        {
          "refId": "B",
          "expr": "sum(increase(applylens_agent_threadlist_returned_total{intent=\"followups\"}[24h]))",
          "legendFormat": "Thread Lists",
          "interval": "",
          "format": "time_series"
        },
        {
          "refId": "C",
          "expr": "sum(increase(applylens_agent_thread_to_tracker_click_total{intent=\"followups\"}[24h]))",
          "legendFormat": "Clicks",
          "interval": "",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              }
            ]
          },
          "mappings": []
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Runs"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "blue"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Thread Lists"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "yellow"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Clicks"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "horizontal",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "showPercentageChange": false
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 18
      }
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Bills – runs, thread_lists, clicks (24h)",
      "description": "Funnel metrics for the 'bills' intent over the last 24 hours.",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(applylens_agent_runs_total{intent=\"bills\"}[24h]))",
