        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (host_family, segment_key, source) (increase(applylens_autofill_style_choice_total[$__range]))",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Value"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Count"
              },
              {
                "id": "custom.displayMode",
                "value": "color-background"
              },
              {
                "id": "color",
                "value": {
                  "mode": "thresholds"
                }
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "value": null,
                      "color": "transparent"
                    },
                    {
                      "value": 1,
                      "color": "light-blue"
                    },
                    {
                      "value": 10,
                      "color": "blue"
                    },
                    {
                      "value": 50,
                      "color": "dark-blue"
                    }
                  ]
                }
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Count"
          }
        ]
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "indexByName": {
              "host_family": 0,
              "segment_key": 1,
              "source": 2,
              "Value": 3
            },
            "renameByName": {
              "host_family": "ATS Family",
              "segment_key": "Segment",
              "source": "Source",
              "Value": "Count"
            }
          }
        }
